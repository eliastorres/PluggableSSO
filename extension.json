{
	"name": "PluggableSSO",
	"namemsg": "pluggablesso-extensionname",
	"version": "0.1",
	"author": [
		"[//mwstake.org Mark A. Hershberger]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:PluggableSSO",
	"descriptionmsg": "pluggablesso-desc",
	"type": "authentication",
	"license-name": "GPL-3.0+",
	"MessagesDirs": {
		"PluggableSSO": [
			"i18n"
		]
	},
	"Hooks": {
		"AuthPluginSetup": [
			"PluggableSSO\\SSOHooks::onAuthPluginSetup"
		]
	},
	"AutoloadClasses": {
		"PluggableSSO\\PluggableSSO": "PluggableSSO.class.php",
		"PluggableSSO\\SSOHooks": "PluggableSSO.hooks.php",
		"PluggableSSO\\Auth":  "PluggableSSOAuth.php"
	},
	"config": {
		"ExtensionFunctions": [
			"PluggableSSO\\SSOHooks::initExtension"
		],
		"SSOHeader": "REMOTE_USER",
		"AuthRemoteuserDomain": "example.wiki"
	},
	"manifest_version": 1
}
